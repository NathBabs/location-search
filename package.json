{
  "name": "location-search",
  "version": "1.0.0",
  "description": "Query for location",
  "main": "index.js",
  "scripts": {
    "build": "rimraf build && npx tsc",
    "dev": "npm run build && concurrently \"npx tsc --watch\" \"nodemon --es-module-specifier-resolution=node -q build/src/server.js\"",
    "start": "ts-node-dev --respawn --transpile-only src/server.ts",
    "lint": "eslint --ignore-path .eslintignore . --ext .ts",
    "lint:fix": "eslint --ignore-path .eslintignore . --ext .ts --fix",
    "format": "prettier --ignore-path .prettierignore --write \"**/*.+(ts|json)\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Nathaniel Babalola",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "csvtojson": "^2.0.10",
    "dayjs": "^1.11.3",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "mongodb": "^4.7.0",
    "mongoose": "^6.4.1",
    "morgan": "^1.10.0",
    "pino": "^8.1.0",
    "pino-pretty": "^8.1.0",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@typescript-eslint/eslint-plugin": "^5.30.3",
    "@typescript-eslint/parser": "^5.30.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "nodemon": "^2.0.18",
    "prettier": "^2.7.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
